{
  "name": "wink-snippets",
  "displayName": "wink-snippets",
  "description": "vue3+ts常用代码片段",
  "version": "1.0.0",
  "author": {
    "name": "向文可",
    "email": "13202090601@163.com",
    "url": "https://xiangwenke.love"
  },
  "publisher": "xwink",
  "icon": "icon.ico",
  "repository": {
    "type": "github",
    "url": "https://github.com/x-wink/wink-snippets"
  },
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "vue",
        "aliases": [
          "Vue",
          "vue"
        ],
        "extensions": [
          ".vue"
        ]
      }
    ],
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "html",
        "path": "./snippets/html.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue.json"
      }
    ]
  },
  "scripts": {
    "build": "vsce package",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "release": "bumpp package.json --commit \"release: v%s\" --push --all --tag && pnpm run build && vsce publish"
  },
  "devDependencies": {
    "@vscode/vsce": "^2.19.0",
    "bumpp": "^9.1.0",
    "conventional-changelog-cli": "^2.2.2"
  }
}
